{
    "arrowParens": "always",
	"jsxSingleQuote": false,
	"overrides": [
		{
			"files": ".*rc",
			"options": { "parser": "json" }
		}
	],
	"plugins": ["prettier-plugin-tailwindcss"],
	"printWidth": 80,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true
}